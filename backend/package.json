{
  "name": "pg-curricula-backend",
  "version": "1.2.0",
  "engines": {
    "pnpm": ">=9.0.0"
  },
  "_moduleAliases": {
    "@": "dist",
    "@/data": "data"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate:previaturas": "cd src/scripts && ts-node generarPreviaturasJson.ts",
    "generate:unidades-curriculares": "cd src/scripts && ts-node generarUnidadesCurricularesJson.ts",
    "generate:ucs-grupos": "cd src/scripts && ts-node generarUCsGruposJson.ts",
    "generate:ucs-grupos-actuales": "cd src/scripts && ts-node generarUCsGruposActualesJson.ts"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@jest/globals": "^29.7.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.0.3",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}
